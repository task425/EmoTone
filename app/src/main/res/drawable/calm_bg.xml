<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item
        android:width="40dp"
        android:height="40dp"
        android:top="5dp">
        <shape android:shape="oval">
            <solid android:color="@color/yellowgreen_dark" />
        </shape>
    </item>

    <item
        android:width="40dp"
        android:height="40dp"
        android:gravity="right"
        android:top="5dp">
        <shape android:shape="oval">
            <solid android:color="@color/yellowgreen_dark" />
        </shape>
    </item>


    <item
        android:height="40dp"
        android:left="20dp"
        android:right="20dp"
        android:top="5dp">
        <shape>
            <solid android:color="@color/yellowgreen_dark" />

        </shape>
    </item>


