    <item
        android:width="40dp"
        android:height="40dp"
        android:left="0dp">
        <shape android:shape="oval">
            <solid android:color="@color/yellowgreen" />
        </shape>
    </item>

    <item


        android:width="40dp"
        android:height="40dp"
        android:gravity="right">
        <shape android:shape="oval">
            <solid android:color="@color/yellowgreen" />
        </shape>
    </item>


    <item
        android:height="40dp"
        android:left="20dp"
        android:right="25dp">
        <shape>
            <solid android:color="@color/yellowgreen" />

        </shape>
